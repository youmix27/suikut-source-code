<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:suikut.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="suikut.Views.MainMenuView"
             x:DataType="vm:MainMenuViewModel">
    
    <StackPanel Margin="20" VerticalAlignment="Center">
        <TextBlock FontSize="30"
                   Margin="10,10,10,10"
                   HorizontalAlignment="Center">SUIKUT</TextBlock>
        <Button Margin="10"
                Width="250"
                HorizontalAlignment="Center"
                HorizontalContentAlignment="Center"
                x:CompileBindings="False"
                Command="{Binding $parent[Window].DataContext.GoLevelMenu}">Jouer</Button>
        <Button Margin="10"
                Width="250"
                HorizontalAlignment="Center"
                HorizontalContentAlignment="Center">Paramètre</Button>
        <Button Margin="10"
                Width="250"
                HorizontalAlignment="Center"
                HorizontalContentAlignment="Center"
                x:CompileBindings="False"
                Command="{Binding $parent[Window].DataContext.GoLogin}">Se Déconnecter</Button>
        
    </StackPanel>
    
    
</UserControl>
